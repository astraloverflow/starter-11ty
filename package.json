{
  "private": true,
  "license": "",
  "version": "2023.04.04",
  "name": "@astraloverflow/starter-11ty",
  "description": "A starter template for static generated blogs using 11ty",
  "scripts": {
    "test": "npx stylelint \"src/**/*.{css,scss}\" && npx eslint \"src/**/*.{js}\"",
    "format": "npx prettier --write",
    "dev": "npx @11ty/eleventy --serve --incremental",
    "prod": "npx cross-env NODE_ENV=production npx @11ty/eleventy"
  },
  "browserslist": [
    "last 3 major versions and not dead and supports es6-module"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "eleventy-plugin-rev": "^1.1.1",
    "eleventy-plugin-time-to-read": "^1.3.0",
    "eleventy-sass": "^2.2.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "html-minifier": "^4.0.0",
    "markdown-it-anchor": "^8.6.7",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "stylelint-order": "^6.0.3",
    "toml": "^3.0.0"
  }
}
